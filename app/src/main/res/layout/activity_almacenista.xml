<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activityAlmacenista"
    android:background="@drawable/fondomenu"
    android:fitsSystemWindows="true">

    <LinearLayout
        android:layout_width="1019dp"
        android:layout_height="705dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="182dp"
            android:layout_height="151dp"
            android:layout_marginTop="115dp"
            app:srcCompat="@drawable/teamwork" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:fontFamily="@font/amiko_bold"
            android:text="Almacenista"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="25sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="565dp"
            android:gravity="center"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="85dp"
                android:layout_marginTop="100dp"
                android:orientation="horizontal">

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="159dp"
                android:orientation="horizontal">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/btnEntradaInsumos"
                        android:layout_width="153dp"
                        android:layout_height="91dp"
                        android:layout_marginEnd="24dp"
                        android:background="@drawable/rounded_buttom"
                        android:backgroundTint="@color/blue"
                        android:drawableLeft="@drawable/health_check"
                        android:padding="10dp"
                        android:text="Entrada insumos"
                        android:textColor="@color/white"
                        android:textSize="10sp"
                        app:layout_constraintEnd_toStartOf="@+id/btnBuscarInsumo"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Button
                        android:id="@+id/btnGenerarReporteInsumos"
                        android:layout_width="153dp"
                        android:layout_height="91dp"
                        android:layout_marginStart="24dp"
                        android:background="@drawable/rounded_buttom"
                        android:backgroundTint="@color/blue"
                        android:drawableLeft="@drawable/prueba"
                        android:padding="10dp"
                        android:text="Reporte "
                        android:textColor="@color/white"
                        android:textSize="10sp"
                        app:layout_constraintStart_toEndOf="@+id/btnBuscarInsumo"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Button
                        android:id="@+id/btnBuscarInsumo"
                        android:layout_width="153dp"
                        android:layout_height="91dp"
                        android:background="@drawable/rounded_buttom"
                        android:backgroundTint="@color/blue"
                        android:drawableLeft="@drawable/seon"
                        android:padding="10dp"
                        android:text="Inventario"
                        android:textColor="@color/white"
                        android:textSize="10sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="104dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="100dp"
                android:orientation="horizontal"

                >

                <Button
                    android:id="@+id/btnMenu"
                    android:layout_width="114dp"
                    android:layout_height="66dp"
                    android:layout_marginLeft="550dp"
                    android:layout_marginTop="23dp"
                    android:background="@drawable/rounded_buttom"
                    android:backgroundTint="@color/orange"
                    android:drawableLeft="@drawable/house"
                    android:padding="20dp"
                    android:text="Menu"
                    android:textColor="@color/white"
                    android:textSize="10dp" />

                <Button
                    android:id="@+id/btnAjustes"
                    android:layout_width="117dp"
                    android:layout_height="66dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="23dp"
                    android:background="@drawable/rounded_buttom"
                    android:backgroundTint="@color/orange"
                    android:drawableLeft="@drawable/engranaje"
                    android:padding="10dp"
                    android:text="Ajuustes"
                    android:textColor="@color/white"
                    android:textSize="10dp" />

                <Button
                    android:id="@+id/btnLogaout"
                    android:layout_width="114dp"
                    android:layout_height="66dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="23dp"
                    android:background="@drawable/rounded_buttom"
                    android:backgroundTint="@color/orange"
                    android:drawableLeft="@drawable/logout"
                    android:padding="20dp"
                    android:text="Salir"
                    android:textColor="@color/white"
                    android:textSize="10dp" />

            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>